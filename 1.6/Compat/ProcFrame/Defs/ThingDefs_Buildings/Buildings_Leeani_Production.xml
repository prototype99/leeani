<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BenchBase">
		<defName>LeeaniWinePress</defName>
		<label>wine press</label>
		<thingClass>Building_WorkTable</thingClass>
		<description>A manual foot-press to squash berries and fruit into a juice, ready for fermenting.</description>		
		<graphicData>
			<texPath>Things/Building/Production/CiderPress</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1.2,2.4)</drawSize>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
		</graphicData>
		<costList>
			<WoodLog>50</WoodLog>
			<Steel>30</Steel>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<defaultPlacingRot>South</defaultPlacingRot>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<WorkToBuild>2000</WorkToBuild>
			<MaxHitPoints>180</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<size>(1,2)</size>
		<designationCategory>Production</designationCategory>
		<passability>Standable</passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellIcon />
		<interactionCellOffset>(0,0,0)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<recipes>
			<li>MakeBerryJuice</li>
			<li>MakeGrapeJuice</li>
		</recipes>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<researchPrerequisites>
			<li>LeeaniBrewing</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>LeeaniCiderKeg</defName>
		<label>cider keg</label>
		<thingClass>Building_WorkTable</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>		
		<graphicData>
			<texPath>Things/Building/Production/CiderKeg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.2,1.2)</drawSize>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.50</fillPercent>
		<pathCost>60</pathCost>
		<building>
			<isMealSource>true</isMealSource>
		</building>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<description>A specially-made keg for brewing cider from berries.</description>
		<costList>
			<Steel>10</Steel>
			<WoodLog>30</WoodLog>
		</costList>
		<comps>
			<li Class="CompProperties_Forbiddable"/>			
			 <li Class="ProcessorFramework.CompProperties_Processor">
    <capacity>25</capacity>
    <independentProcesses>true</independentProcesses>
    <parallelProcesses>true</parallelProcesses>
    <dropIngredients>false</dropIngredients>
    <showProductIcon>true</showProductIcon>
    <barOffset>(0.0, 0.25)</barOffset>
    <barScale>(1.0, 1.0)</barScale>
    <colorCoded>false</colorCoded>
    <processes>     
	<li>LeeaniCiderProc</li>	
    </processes>
  </li>
		</comps>
		<tickerType>Normal</tickerType>
		<rotatable>false</rotatable>
		<designationCategory>Production</designationCategory>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<constructEffect>ConstructWood</constructEffect>
		<researchPrerequisites>
			<li>LeeaniBrewing</li>
		</researchPrerequisites>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>LeeaniWineKeg</defName>
		<label>wine keg</label>
		<thingClass>Building_WorkTable</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>		
		<graphicData>
			<texPath>Things/Building/Production/WineKeg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.2,1.2)</drawSize>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.50</fillPercent>
		<pathCost>60</pathCost>
		<building>
			<isMealSource>true</isMealSource>
		</building>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<description>A specially-made keg for brewing wine from grape juice.</description>
		<costList>
			<Steel>10</Steel>
			<WoodLog>30</WoodLog>
		</costList>
		<comps>
			<li Class="CompProperties_Forbiddable"/>			
			 <li Class="ProcessorFramework.CompProperties_Processor">
    <capacity>25</capacity>
    <independentProcesses>true</independentProcesses>
    <parallelProcesses>true</parallelProcesses>
    <dropIngredients>false</dropIngredients>
    <showProductIcon>true</showProductIcon>
    <barOffset>(0.0, 0.25)</barOffset>
    <barScale>(1.0, 1.0)</barScale>
    <colorCoded>false</colorCoded>
    <processes>
      <li>LeeaniWineProc</li>	
    </processes>
  </li>
		</comps>
		<tickerType>Normal</tickerType>
		<rotatable>false</rotatable>
		<designationCategory>Production</designationCategory>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<constructEffect>ConstructWood</constructEffect>
		<researchPrerequisites>
			<li>LeeaniWineBrewing</li>
		</researchPrerequisites>
	</ThingDef>

</Defs>