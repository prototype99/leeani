<Defs>

<ProcessorFramework.ProcessDef>
  <defName>LeeaniWineProc</defName>
  <thingDef>LeeaniWine</thingDef>
  <ingredientFilter>
    <thingDefs>
      <li>GrapeJuice</li>
    </thingDefs>
  </ingredientFilter>
  <processDays>6</processDays> <!-- in days, a day is 60,000 ticks -->
